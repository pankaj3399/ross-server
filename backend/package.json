{
  "name": "matur-ai-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "seed": "ts-node src/scripts/seeds/index.ts",
    "migrate": "npx node-pg-migrate up",
    "db:init": "npx ts-node -r dotenv/config src/utils/database.ts && npm run migrate && npm run seed"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@mastra/evals": "^0.14.2",
    "@types/nodemailer": "^7.0.2",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "inngest": "^3.48.1",
    "jsonwebtoken": "^9.0.2",
    "node-pg-migrate": "^8.0.3",
    "nodemailer": "^7.0.9",
    "pg": "^8.11.3",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "stripe": "^14.21.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.30",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
